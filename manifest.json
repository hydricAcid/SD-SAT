{
    "manifest_version": 2,
    "name": "first-extension",
    "version": "1.0.0",
    "description": "test extension",

    "content_scripts": [
        {
            "matches": [
                "https://learn.crcs.vic.edu.au/*"
            ],
            "js": ["test.js"]
        }
    ],
    "icons": {
        "128": "images/icontest.png"
    },
    "web_accessible_resources": ["images/*", "injected.css"],
    "browser_action": {
        "default_popup": "index.html"
    }
    
}


